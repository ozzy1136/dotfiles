{
	"$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.33/schema.json",
	"app_specific_configuration_path": "$Env:KOMOREBI_CONFIG_HOME/applications.json",
	"default_workspace_padding": 10,
	"default_container_padding": 10,
	"border": true,
	"theme": {
		"palette": "Catppuccin",
		"name": "Mocha",
		"unfocused_border": "Overlay0",
		"bar_accent": "Rosewater"
	},
	"stackbar": {
		"height": 40,
		"mode": "OnStack",
		"tabs": {
			"width": 300
		}
	},
	"monitors": [
		{
			"workspaces": [
				{
					"name": "I",
					"layout": "BSP"
				},
				{
					"name": "II",
					"layout": "BSP"
				},
				{
					"name": "III",
					"layout": "BSP"
				},
				{
					"name": "IV",
					"layout": "BSP"
				},
				{
					"name": "V",
					"layout": "BSP"
				},
				{
					"name": "VI",
					"layout": "BSP"
				}
			]
		}
	],
	"floating_applications": [
		[
			{
				"kind": "Exe",
				"id": "chrome.exe",
				"matching_strategy": "Equals"
			},
			{
				"kind": "Title",
				"id": "_crx",
				"matching_strategy": "StartsWith"
			}
		]
	]
}
